{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "appData": {
    "version": "2025.08.01",
    "name": "Moja Miriam"
  },
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/browserconfig.xml",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2|json)"
        ]
      }
    },
    {
      "name": "assetsAlwaysNeeded",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/cldr-data/**", "/i18n/**"]
      }
    },
    {
      "name": "icons",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/icons/**"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "freshness-from-api",
      "version": 2,
      "urls": [
        "/meetings",
        "/mission",
        "/diaconie",
        "/structure",
        "/regulations",
        "/calendar/events",
        "/announcements/current"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 20,
        "maxAge": "1h",
        "timeout": "3s"
      }
    }
  ]
}
